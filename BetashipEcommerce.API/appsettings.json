{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Redis": "localhost:6379,abortConnect=false"
  },
  "Hangfire": {
    "OutboxProcessingInterval": "00:00:30",
    "CleanupRetentionDays": 30
  },
  "CartCache": {
    "DefaultExpiryDays": 30,
    "AbandonedThresholdHours": 24,
    "SyncIntervalHours": 6
  }
}